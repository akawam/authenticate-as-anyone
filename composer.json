{
    "name": "akawam/authenticate-as-anyone",
    "description": "Package that allows you to authenticate as anyone, see readme for more informations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin",
            "email": "valentin.estreguil@akawam.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Akawam\\AuthenticateAsAnyone\\AuthenticateAsAnyoneServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Akawam\\AuthenticateAsAnyone\\": "src",
            "Akawam\\AuthenticateAsAnyone\\Tests\\": "tests"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "9.5.x-dev"

    }
}
